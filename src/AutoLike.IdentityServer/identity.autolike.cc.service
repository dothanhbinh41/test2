ConditionPathExists=/root/Autolike.v8/identity.autolike.cc
After=network.target

[Service]
Type=simple
User=root
Group=root
WorkingDirectory=/root/Autolike.v8/identity.autolike.cc
ExecStart=/root/Autolike.v8/identity.autolike.cc/identity.autolike.cc
